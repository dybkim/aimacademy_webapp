<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="courseService" name="courseService" class="org.mockito.Mockito" factory-method="mock">
        <constructor-arg value="com.aimacademyla.service.CourseService"/>
    </bean>

    <bean id="memberService" name="memberService" class="org.mockito.Mockito" factory-method="mock">
        <constructor-arg value="com.aimacademyla.service.MemberService"/>
    </bean>

    <bean id="studentRegistrationService" name="studentRegistrationService" class="org.mockito.Mockito" factory-method="mock">
        <constructor-arg value="com.aimacademyla.service.StudentRegistrationService"/>
    </bean>


</beans>